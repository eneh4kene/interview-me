# API Environment Variables Example
PORT=3001
NODE_ENV=development

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/interviewsfirst

# Redis
REDIS_URL=redis://localhost:6379

# JWT Keys (paths or inline)
JWT_PRIVATE_KEY_PATH=./private.pem
JWT_PUBLIC_KEY_PATH=./public.pem

# Aggregators
ADZUNA_APP_ID=
ADZUNA_APP_KEY=
JOOBLE_API_KEY=

# n8n Automation
N8N_AI_APPLY_WEBHOOK_URL=http://localhost:5678/webhook/ai-apply
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin

# Stripe
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Rate Limits (requests per minute)
ADZUNA_RATE_LIMIT_PER_MINUTE=25
JOOBLE_RATE_LIMIT_PER_MINUTE=60

# Job Cache Settings
JOB_CACHE_TTL_SECONDS=1800 # 30 minutes
JOB_STORAGE_TTL_DAYS=30 # Delete jobs older than 30 days 